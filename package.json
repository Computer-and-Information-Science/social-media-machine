        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vitest": "^0.34.6"
    },
    "eslintConfig": {
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2020,
            "ecmaFeatures": {
                "jsx": true
