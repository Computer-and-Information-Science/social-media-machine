        "lz-string": "^1.5.0",
        "peerjs": "^1.5.1",
        "qrcode": "^1.5.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^13.4.0",
        "react-router": "^6.18.0",
        "react-router-dom": "^6.18.0",
        "recoil": "^0.7.7",
